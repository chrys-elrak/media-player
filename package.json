{
  "name": "media-player",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:start": "nodemon --watch app --exec electron .",
    "ng:start": "ng build --output-path app --watch",
    "electron-build": "ng build --base-href ./ && electron .",
    "build-win": "electron-packager . media-player-win --overwrite --asar --platform=win32 --arch=ia32 --icon=src/assets/media-player.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Chrys Media Player\"",
    "build-linux": "electron-packager . media-player-linux --overwrite --asar --platform=linux --arch=x64 --icon=src/assets/media-player.png --prune=true --out=release-builds",
    "build-dpkg": "electron-installer-debian --src release-builds/media-player-linux-linux-x64 --arch x64 --config config.debian.json"
  },
  "description": "A little video and audio player",
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "@capacitor/angular": "^1.0.3",
    "@capacitor/core": "latest",
    "@capacitor/electron": "^2.4.6",
    "@ionic/angular": "^5.5.4",
    "@types/electron": "^1.6.10",
    "dotenv": "^8.2.0",
    "mat-video": "^2.8.1",
    "moment": "^2.27.0",
    "ngx-electron": "^2.2.0",
    "path": "^0.12.7",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.9",
    "@angular/cli": "~9.1.9",
    "@angular/compiler-cli": "~9.1.11",
    "@capacitor/cli": "latest",
    "@ionic/angular-toolkit": "latest",
    "@types/core-js": "^2.5.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.47",
    "codelyzer": "^5.1.2",
    "electron": "^11.5.0",
    "electron-installer-debian": "^3.1.0",
    "electron-packager": "^15.0.0",
    "electron-reload": "^1.5.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "nodemon": "^2.0.7",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
